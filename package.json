{
  "name": "duxtree",
  "version": "1.0.0",
  "description": "ReactJS tree component with custom rendering and dynamic loading",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "testbed": "./node_modules/.bin/webpack-dev-server --env=testbed",
